{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/api/404.js", "pages/src/component/Layout/footer/footer.js", "pages/src/component/Layout/Layout.js", "pages/sex.js", "pages/relationship.js", "pages/psychology.js", "pages/404.js", "pages/src/component/screens/home/home.js", "pages/connection.js"],
  "exclude": ["node_modules"]
}
